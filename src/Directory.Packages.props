    <PackageVersion Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.0-preview.6.24327.7" />
    <PackageVersion Include="Microsoft.Extensions.Resilience" Version="9.0.0-preview.6.24353.1" />
